ALLOCATE GLOBAL Integer  { I_COL, I_ROW, I_NEW }
ALLOCATE GLOBAL String   { STRVAL, STRING, OUTPUT }
ALLOCATE GLOBAL Boolean  { B_COMPVAL }
ALLOCATE GLOBAL IntArray { A_NUMBERS }
ALLOCATE GLOBAL StrArray { L_LIST }

-spath ../input
-sfile NewTest.ods
-load 1 false
-tab 0

# -pfile chase-3996-2025-04.pdf
-clip
PRINT $RESPONSE

I_COL = 10
STRVAL = 757
I_ROW = ($STRVAL % 25) - (40 + (6 * ($I_COL + 5) / 10)) * -3 - 8
STRING = "some  funny  words   HAHA"
L_LIST = { hello, world, what, is, up? }
A_NUMBERS = { 143, -22, 777, 0xFFFF, 999, 222, 87654, -12 }

B_COMPVAL = $I_COL < (10 * $STRVAL) - 20
B_COMPVAL = $STRING == "some funny"
B_COMPVAL = $STRING == "some  funny  words   HAHA"
B_COMPVAL = $STRING > "some funny"
B_COMPVAL = $I_COL < $STRVAL % 25

-default 1 false
CLEAR RESPONSE
-colget 0 2 64
PRINT $RESPONSE
CLEAR RESPONSE

STRING += " more stuff"
I_NEW *= $I_ROW *3

INSERT L_LIST "he said:"
APPEND L_LIST "another entry"
MODIFY L_LIST 2 CHARLIE
REMOVE L_LIST 4
TRUNCATE L_LIST 3
POP L_LIST

I_COL = $STRING.SIZE * 2 - 60
STRING = $STRING.UPPER
STRING = $STRING + " " + $L_LIST[0]

FOR LOOPIX = 0 UPTO 9
  I_ROW = $A_NUMBERS.SIZE + 12
  IF $I_ROW <= 14
    OUTPUT = "SML: " + $I_ROW
  ELSEIF $I_ROW >= ($STRVAL / 70) + 8
    OUTPUT = "BIG: " + $I_ROW
  ELSE
    OUTPUT = "MID: " + $I_ROW
  ENDIF
  PRINT $OUTPUT
  IF ! $A_NUMBERS.ISEMPTY
    POP A_NUMBERS
  ENDIF
NEXT

-save

ENDMAIN

